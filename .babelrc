{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 1 versions",
          "IE >= 11"
        ],
        "useBuiltIns": true,
        "node": "current"
      }
    }],
    ["flow"]
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "add-module-exports",
    "transform-es2015-for-of"
  ]
}
