{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "last 1 versions",
          "IE >= 11"
        ]
      },
      "useBuiltIns": "entry"
    }],
    ["@babel/preset-flow"]
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-transform-for-of",
    ["@babel/plugin-proposal-decorators", {"decoratorsBeforeExport": true}],
    "@babel/plugin-proposal-class-properties"
  ]
}
